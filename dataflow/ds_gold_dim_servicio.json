{
	"name": "ds_gold_dim_servicio",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_llamadas",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_gold_dim_servicio",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          servicio as string,",
				"          registro as string,",
				"          numero_a as string,",
				"          imei_a as string,",
				"          rbs_utilizada_a as string,",
				"          celda_a as string,",
				"          latitud_a as decimal(12,6),",
				"          longitud_a as decimal(12,6),",
				"          numero_b as string,",
				"          imei_b as string,",
				"          rbs_utilizada_b as string,",
				"          celda_b as string,",
				"          latitud_b as decimal(12,6),",
				"          longitud_b as decimal(12,6),",
				"          fecha as timestamp,",
				"          duracion as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 select(mapColumn(",
				"          servicio",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 aggregate(groupBy(servicio),",
				"     each(match(name!=\"servicio\"), $$ = first($$))) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_servicio as integer,",
				"          servicio as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          servicio",
				"     )) ~> sink1"
			]
		}
	}
}
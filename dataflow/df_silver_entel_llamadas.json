{
	"name": "df_silver_entel_llamadas",
	"properties": {
		"folder": {
			"name": "Silver"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_bronze_llamadas",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_llamadas",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "cast1"
				},
				{
					"name": "derivedColumn2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          servicio as string,",
				"          registro as string,",
				"          numero_a as string,",
				"          imei_a as string,",
				"          rbs_utilizada_a as string,",
				"          celda_a as string,",
				"          latitud_longitud_a as string,",
				"          numero_b as string,",
				"          imei_b as string,",
				"          rbs_utilizada_b as string,",
				"          celda_b as string,",
				"          latitud_longitud_b as string,",
				"          fecha as string,",
				"          duracion as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 derive(fecha = trim(fecha) ,",
				"          latitud_a = replace(\r",
				"    split(\r",
				"        replace(\r",
				"            replace(\r",
				"                replace(trim(latitud_longitud_a), '\"',''),\r",
				"                ' ', ''\r",
				"            ),\r",
				"            ',-', '|-'\r",
				"        ),\r",
				"        '|'\r",
				"    )[1],\r",
				"    ',', '.'\r",
				"),",
				"          longitud_a = replace(\r",
				"    split(\r",
				"        replace(\r",
				"            replace(\r",
				"                replace(trim(latitud_longitud_a), '\"',''),\r",
				"                ' ', ''\r",
				"            ),\r",
				"            ',-', '|-'\r",
				"        ),\r",
				"        '|'\r",
				"    )[2],\r",
				"    ',', '.'\r",
				"),",
				"          latitud_b = replace(\r",
				"    split(\r",
				"        replace(\r",
				"            replace(\r",
				"                replace(trim(latitud_longitud_b), '\"',''),\r",
				"                ' ', ''\r",
				"            ),\r",
				"            ',-', '|-'\r",
				"        ),\r",
				"        '|'\r",
				"    )[1],\r",
				"    ',', '.'\r",
				"),",
				"          longitud_b = replace(\r",
				"    split(\r",
				"        replace(\r",
				"            replace(\r",
				"                replace(trim(latitud_longitud_b), '\"',''),\r",
				"                ' ', ''\r",
				"            ),\r",
				"            ',-', '|-'\r",
				"        ),\r",
				"        '|'\r",
				"    )[2],\r",
				"    ',', '.'\r",
				"),",
				"          duracion = toTimestamp(   iif(     isNull(duracion) || trim(toString(duracion)) == '' || toString(duracion) == 'NULL' || toString(duracion) == '-',     '00:00:00',     toString(duracion)   ),   'HH:mm:ss' ),",
				"          numero_b = iif(endsWith(toString(numero_b), '.0'),     replace(toString(numero_b), '.0', ''),     toString(numero_b))) ~> derivedColumn1",
				"derivedColumn1 cast(output(",
				"          servicio as string,",
				"          fecha as timestamp 'dd-MM-yyyy HH:mm:ss',",
				"          duracion as timestamp 'HH:mm:ss',",
				"          latitud_a as decimal(12,6) '000,000,000.000',",
				"          longitud_a as decimal(12,6) '000,000,000.000',",
				"          latitud_b as decimal(12,6) '000,000,000.000',",
				"          longitud_b as decimal(12,6) '000,000,000.000'",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 derive(latitud_a = iif(isNull(latitud_a), toDecimal(-99.999999), latitud_a),",
				"          latitud_b = iif(isNull(latitud_b), toDecimal(-99.999999), latitud_b),",
				"          longitud_a = iif(isNull(longitud_a), toDecimal(-99.999999), longitud_a),",
				"          longitud_b = iif(isNull(longitud_b), toDecimal(-99.999999), longitud_b),",
				"          imei_a = trim(imei_a),",
				"          imei_b = trim(imei_b)) ~> derivedColumn2",
				"derivedColumn2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          servicio as string,",
				"          registro as string,",
				"          numero_a as string,",
				"          imei_a as string,",
				"          rbs_utilizada_a as string,",
				"          celda_a as string,",
				"          latitud_a as decimal(12,6),",
				"          longitud_a as decimal(12,6),",
				"          numero_b as string,",
				"          imei_b as string,",
				"          rbs_utilizada_b as string,",
				"          celda_b as string,",
				"          latitud_b as decimal(12,6),",
				"          longitud_b as decimal(12,6),",
				"          fecha as timestamp,",
				"          duracion as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          servicio,",
				"          registro,",
				"          numero_a,",
				"          imei_a,",
				"          rbs_utilizada_a,",
				"          celda_a,",
				"          latitud_a,",
				"          longitud_a,",
				"          numero_b,",
				"          imei_b,",
				"          rbs_utilizada_b,",
				"          celda_b,",
				"          latitud_b,",
				"          longitud_b,",
				"          fecha,",
				"          duracion",
				"     )) ~> sink1"
			]
		}
	}
}
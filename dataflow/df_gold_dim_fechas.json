{
	"name": "df_gold_dim_fechas",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_llamadas",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_datos_silver",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_gold_dim_fechas",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "select2"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "union1"
				},
				{
					"name": "select3"
				},
				{
					"name": "union2"
				},
				{
					"name": "sort1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          servicio as string,",
				"          registro as string,",
				"          numero_a as string,",
				"          imei_a as string,",
				"          rbs_utilizada_a as string,",
				"          celda_a as string,",
				"          latitud_a as decimal(12,6),",
				"          longitud_a as decimal(12,6),",
				"          numero_b as string,",
				"          imei_b as string,",
				"          rbs_utilizada_b as string,",
				"          celda_b as string,",
				"          latitud_b as decimal(12,6),",
				"          longitud_b as decimal(12,6),",
				"          fecha as timestamp,",
				"          duracion as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source(output(",
				"          fecha_inicio as timestamp,",
				"          fecha_fin as timestamp,",
				"          numero as string,",
				"          imei as string,",
				"          rbs_utilizada as string,",
				"          celda as string,",
				"          latitud as decimal(12,6),",
				"          longitud as decimal(12,6),",
				"          subidos as integer,",
				"          descargados as integer,",
				"          total as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source2",
				"source1 select(mapColumn(",
				"          fecha",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"source2 select(mapColumn(",
				"          fecha_inicio,",
				"          fecha_fin",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 derive(fecha = fecha_fin) ~> derivedColumn2",
				"derivedColumn2 derive(fecha = fecha_inicio) ~> derivedColumn3",
				"derivedColumn3, derivedColumn2 union(byName: true)~> union1",
				"union1 select(mapColumn(",
				"          fecha",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"select1, select3 union(byName: true)~> union2",
				"union2 sort(asc(fecha, true)) ~> sort1",
				"sort1 aggregate(groupBy(fecha),",
				"     each(match(name!=\"fecha\"), $$ = first($$))) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_fecha as integer,",
				"          fecha as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          fecha",
				"     )) ~> sink1"
			]
		}
	}
}